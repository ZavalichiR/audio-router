# Discord Audio Router Bot - Configuration
# Copy this file to .env and fill in your actual values

# Main bot token (required)
MAIN_BOT_TOKEN=your_main_bot_token_here

# Listener bot tokens (optional - if not provided, main token will be used for all bots)
# You can provide multiple tokens for better performance
LISTENER_BOT_TOKEN=your_listener_bot_token_here
# LISTENER_BOT_TOKENS=token1,token2,token3
# LISTENER_BOT_TOKEN_1=token1
# LISTENER_BOT_TOKEN_2=token2

# Bot configuration
BOT_PREFIX=!
LOG_LEVEL=INFO
